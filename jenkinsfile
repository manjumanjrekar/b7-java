pipeline {
    agent any

    environment {
        DOCKERHUB_CREDENTIALS = credentials('manju256')
        IMAGE_NAME = 'your-dockerhub-username/manju256'
        IMAGE_TAG = 'latest'
    }

    stages {
        stage('Build Docker Image') {
            steps {
                script {
                    docker.build("${jenk2.1.0}:${1.0}")
                }
            }
        }
    }
}
